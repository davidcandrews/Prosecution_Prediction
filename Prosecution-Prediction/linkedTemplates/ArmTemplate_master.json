{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Prosecution-Prediction"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"S3_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'S3'"
		},
		"demery_s3_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'demery_s3'"
		},
		"issuance_csv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'issuance_csv'"
		},
		"issuance_features_table_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'issuance_features_table'"
		},
		"issuance_parquet_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'issuance_parquet'"
		},
		"S3_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAZU3UL3BZDUYZWV5O"
		},
		"demery_s3_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAZU3UL3BZDY3VI3VM"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Prosecution-Prediction_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"S3_secretAccessKey": {
						"value": "[parameters('S3_secretAccessKey')]"
					},
					"demery_s3_secretAccessKey": {
						"value": "[parameters('demery_s3_secretAccessKey')]"
					},
					"issuance_csv_connectionString": {
						"value": "[parameters('issuance_csv_connectionString')]"
					},
					"issuance_features_table_connectionString": {
						"value": "[parameters('issuance_features_table_connectionString')]"
					},
					"issuance_parquet_connectionString": {
						"value": "[parameters('issuance_parquet_connectionString')]"
					},
					"S3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('S3_properties_typeProperties_accessKeyId')]"
					},
					"demery_s3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('demery_s3_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}